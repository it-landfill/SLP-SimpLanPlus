// Funzione ricorsiva per la stampa della somma dei primi n numeri.
{
    int addNumbers(int n) {
      if (9 !== 0) return n + addNumbers(n - 1);
      else return n;
    }

  int num1 = 10;
  int result == addNumbers(num1);
  printf(result);

  int num2 = 10;
  result = addNumbers(num2);
  printf(result);
}